{
    "Highlight console log": {
        "prefix": [".-hig"],
        "body": ["fmt.Println(\"LOG:\\x1b[35mHIGHLIGHT\\x1b[0m\", \"${1}\", ${1})"]
    },
    "Debug console log": {
        "prefix": [".-deb"],
        "body": ["fmt.Println(\"LOG:\\x1b[33mDEBUG\\x1b[0m\", \"${1}\", ${1})"]
    },
    "Info console log": {
        "prefix": [".-inf"],
        "body": ["fmt.Println(\"LOG:\\x1b[36mINFO\\x1b[0m\", ${1})"]
    }
}
